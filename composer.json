{
    "name": "gpslab/compressor",
    "license": "MIT",
    "type": "library",
    "homepage": "https://github.com/gpslab/compressor",
    "description": "File compressor",
    "autoload": {
        "psr-4": {
            "GpsLab\\Component\\Compressor\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GpsLab\\Component\\Compressor\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=5.4.0"
    },
    "require-dev": {
        "symfony/filesystem": "^2.4",
        "phpunit/phpunit": "^4.8.36",
        "scrutinizer/ocular": "~1.3",
        "php-coveralls/php-coveralls": "^1.0"
    },
    "suggest": {
        "ext-zlib": "Support for Gzip compressor",
        "ext-bz2": "Support for Bzip2 compressor",
        "ext-zip": "Support for Zip compressor"
    }
}
